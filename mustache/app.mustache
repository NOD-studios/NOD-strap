<!DOCTYPE html>
<!-- app -->
<!--[if lt IE 7]>      <html class="app loading no-js lt-ie9 lt-ie8 lt-ie7 mti-inactive wf-inactive"> <![endif]-->
<!--[if IE 7]>         <html class="app loading no-js lt-ie9 lt-ie8 mti-inactive wf-inactive"> <![endif]-->
<!--[if IE 8]>         <html class="app loading no-js lt-ie9 mti-inactive wf-inactive"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="app loading no-js mti-inactive wf-inactive"> <!--<![endif]-->
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{{ title }}</title>
    <meta name="description" content="{{ ASSET_VERSION }}">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <script type="text/javascript">
    var App,
        AppLoad = AppLoad || [],
        ENV     = JSON.parse('{{{ envJson }}}'),
        require   = {
          urlArgs : 'v={{ ASSET_VERSION }}',
        }, requireScript;
    </script>
    <script data-main="{{ DIR_JS }}/{{ ASSET_JS }}"
      src="{{ DIR_VENDOR }}/requirejs/require.min.js"
      type="text/javascript"></script>
    <script type="text/javascript">
      if (typeof require.config === 'undefined') {
        requireScript = document.createElement('script');
        requireScript.setAttribute('type', 'text/javascript');
        requireScript.setAttribute('data-main', '{{ DIR_JS }}/{{ ASSET_JS }}');
        requireScript.setAttribute('src', '//cdnjs.cloudflare.com/ajax/libs/require.js/2.1.15/require.min.js');
        document.head.appendChild(requireScript);
      }
    </script>
    <style>.loading { visibility: hidden; opacity: 0; }</style>
    <link rel="shortcut icon" href="{{ basePath }}/favicon.ico">
    <link rel="apple-touch-icon image_src"
      href="{{ basePath }}/apple-touch-icon-precomposed.png">

    <!-- social -->
    <meta itemprop="name" content="{{ title }}">
    <meta itemprop="description" content="{{ description }}">
    <meta itemprop="image" content="https://{{ host }}/{{ DIR_RENDER }}/logo.social.png">
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="{{ title }}">
    <meta name="twitter:description" content="{{ description }}">
    <meta name="twitter:image:src" content="{{ baseUrl }}/{{ DIR_RENDER }}/logo.social.png">
    <meta property="og:url" content="{{ baseUrl }}" />
    <meta property="og:image" content="{{ baseUrl }}/{{ DIR_RENDER }}/logo.social.png" />
    <meta property="og:image:secure_url" content="https://{{ host }}/{{ DIR_RENDER }}/logo.social.png" />
    <meta property="og:description" content="{{ description }}" />
    <meta property="og:site_name" content="{{ title }}" />
    <meta property="fb:page_id" content="{{ API_FB_PAGE_ID }}" />
    <meta property="fb:admins" content="{{ admins }}" />

    <!-- device -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  	<meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
  	<meta name="viewport" content="width=device-width" />
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="{{ DIR_ICON }}/ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">

    <link rel="author" href="humans.txt" />

    <link rel="apple-touch-icon" sizes="57x57" href="{{ DIR_ICON }}/apple-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="{{ DIR_ICON }}/apple-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="{{ DIR_ICON }}/apple-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="{{ DIR_ICON }}/apple-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="{{ DIR_ICON }}/apple-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="{{ DIR_ICON }}/apple-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="{{ DIR_ICON }}/apple-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="{{ DIR_ICON }}/apple-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="{{ DIR_ICON }}/apple-icon-180x180.png">
    <link rel="icon" type="image/png" sizes="192x192"  href="{{ DIR_ICON }}/android-icon-192x192.png">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ DIR_ICON }}/favicon-16x16.png">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ DIR_ICON }}/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="96x96" href="{{ DIR_ICON }}/favicon-96x96.png">
    <link rel="manifest" href="{{ DIR_ICON }}/manifest.json">
  </head>
  <body>
    <!-- app/header -->
    <header class="header loading">
      <hgroup>
        <h1 class="logo"><a href="{{ baseUrl }}">{{ title }}</a></h1>
        <h2>{{ description }}</h2>
      </hgroup>
    </header>
    <script type="text/javascript">
      AppLoad.push('header');
    </script>
    <!--/ app/header -->

    <!-- app/menu -->
    <section class="menu loading">
      <nav>
        <ul>
          <li><a href="#">Menu</a></li>
        </ul>
      </nav>
    </section>
    <script type="text/javascript">
      AppLoad.push('menu');
    </script>
    <!-- app/menu -->

    <!-- app/banner -->
    <section class="banner loading">
      <h3>Banner</h3>
    </section>
    <script type="text/javascript">
      AppLoad.push('banner');
    </script>
    <!--/ app/banner -->

    <!-- app/content -->
    <section class="content loading">
      <p>Content</p>
    </section>
    <script type="text/javascript">
      AppLoad.push('content');
    </script>
    <!--/ app/content -->

    <!-- app/modal -->
    <div id="modal" class="modal loading">
      <p>Modal</p>
    </div>
    <script type="text/javascript">
      AppLoad.push('modal');
    </script>
    <!--/ app/modal -->

    <!-- app/footer -->
    <footer class="footer loading">
      <p>2015 &copy; <a
        target="_blank"
        href="{{ VENDOR_URL }}"
      >{{ VENDOR_NAME }}</a></p>
    </footer>
    <script type="text/javascript">
      AppLoad.push('footer');
    </script>
    <!--/ app/footer -->

    <script type="text/javascript">
      require(['app'], function(app) {
        App = app.init();
      });
    </script>
    <script type="text/javascript">
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m);
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
      ga('create', '{{ API_GOOGLE_ID }}', 'auto');
      ga('send', 'pageview');
    </script>
  </body>
</html>
<!-- /app -->
