    "css-loader": "^0.28.0",
    "debug": "^2.6.8",
    "detect-port": "^1.1.1",
    "dotenv": "4.0.0",
    "eslint": "3.19.0",
    "eslint-config-cleanjs": "^4.0.0",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "1.7.1",
    "eslint-plugin-better": "^0.1.5",
    "eslint-plugin-class-property": "^1.0.3",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react-app": "^1.0.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "fs-extra": "2.1.2",
    "gulp": "^3.9.1",
    "gulp-env": "^0.4.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-git": "^2.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-npm-script-sync": "^1.1.0",
    "gulp-plumber": "^1.1.0",
    "gulp-shell": "^0.6.3",
    "gulp-simple-load-tasks": "^1.0.2",
    "gulp-util": "^3.0.8",
    "gzip-size": "3.0.0",
    "history": "^3.3.0",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "0.17.4",
    "isomorphic-fetch": "^2.2.1",
    "jest": "19.0.2",
    "jsdom": "^9.12.0",
    "json-loader": "0.5.4",
    "koa": "^2.2.0",
    "koa-static": "^3.0.0",
    "lodash.debounce": "^4.0.8",
    "nock": "^9.0.11",
    "nodemon": "^1.11.0",
    "object-assign": "4.1.1",
    "path-exists": "3.0.0",
    "postcss-loader": "1.3.3",
    "promise": "7.1.1",
    "prop-types": "^15.5.6",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-transition-group": "^15.5.2",
    "react-dev-utils": "^0.5.2",
    "react-md-spinner": "^0.2.1",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.8",
    "react-test-renderer": "^15.6.1",
    "reactstrap": "3.*",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-mock-store": "^1.2.2",
    "redux-observable": "^0.14.1",
    "rimraf": "2.6.1",
    "rxjs": "^5.3.0",
    "strip-ansi": "3.0.1",
    "style-loader": "0.16.1",
    "url-loader": "0.5.8",
    "webpack": "1.*.*",
    "webpack-dev-server": "1.*.*",
    "webpack-manifest-plugin": "1.*.*",
    "window-or-global": "^1.0.1"
  },
  "dependencies": {
    "horizon": "^2.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "scripts": {
    "start": "nodemon",
    "default": "gulp default",
    "env-test": "gulp env-test",
    "env-production": "gulp env-production",
    "env-development": "gulp env-development",
    "test": "jest --watch --env=jsdom",
    "test-once": "jest --env=jsdom",
    "build": "gulp build",
    "start-client": "gulp start-client",
    "start-server": "gulp start-server",
    "start-client-production": "gulp start-client-production",
    "start-server-production": "gulp start-server-production",
    "gh-pages": "gulp gh-pages",
    "preversion-git-add": "gulp preversion-git-add",
    "preversion": "gulp preversion",
    "postversion-git-push": "gulp postversion-git-push",
    "postversion-deploy-gh-page": "gulp postversion-deploy-gh-page",
    "postversion": "gulp postversion",
    "postpublish": "gulp postpublish"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "transform-decorators-legacy",
          "add-module-exports"
        ],
        "presets": [
          "react-app",
          "stage-0",
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      },
      "development": {
        "plugins": [
          "transform-decorators-legacy",
          "add-module-exports"
        ],
        "presets": [
          "react-app",
          "stage-0",
          [
            "env",
            {
              "browsers": [
                "last 2 versions"
              ],
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      },
      "production": {
        "plugins": [
          "transform-decorators-legacy",
          "add-module-exports"
        ],
        "presets": [
          "react-app",
          "stage-0",
          [
            "env",
            {
              "browsers": [
                "last 2 versions"
              ],
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "ignore": [
      "./config/**/.js"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "react-app",
      "class-property"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "react-app",
      "cleanjs"
    ],
    "rules": {
      "class-property/class-property-semicolon": [
        2,
        "never"
      ],
      "key-spacing": [
        2,
        {
          "afterColon": true,
          "beforeColon": true,
          "align": {
            "beforeColon": true,
            "afterColon": true,
            "on": "colon"
          }
        }
      ],
      "curly": [
        1,
        "multi"
      ],
      "semi": [
        1,
        "never"
      ],
      "indent": [
        "error",
        2
      ],
      "react/jsx-indent": [
        2,
        2
      ],
      "react/jsx-curly-spacing": [
        2,
        "always"
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "array-bracket-spacing": [
        2,
        "always"
      ],
      "template-curly-spacing": [
        2,
        "always"
      ],
      "no-console": 1,
      "fp/no-nil": 0,
      "fp/no-this": 0,
      "fp/no-class": 0,
      "better/no-ifs": 1,
      "better/no-new": 0,
      "fp/no-throw": 0,
      "fp/no-rest-parameters": 0,
      "fp/no-unused-expression": 1,
      "better/explicit-return": 1
    }
  }
}
